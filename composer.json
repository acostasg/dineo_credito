{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "bornfreee/tactician-domain-events": "dev-master",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-migrations-bundle": "3.3.x-dev",
    "doctrine/orm": "^2.9",
    "monolog/monolog": "^2.2",
    "pagerfanta/pagerfanta": "*",
    "phpdocumentor/reflection-docblock": "5.x-dev",
    "phpstan/phpdoc-parser": "1.22.x-dev",
    "sensio/framework-extra-bundle": "^6.2",
    "sonata-project/admin-bundle": "*",
    "sonata-project/doctrine-orm-admin-bundle": "5.x-dev",
    "symfony/asset": "^6.3",
    "symfony/browser-kit": "^6.3",
    "symfony/cache": "^6.3",
    "symfony/console": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/dotenv": "^6.3",
    "symfony/event-dispatcher": "^6.3",
    "symfony/expression-language": "^6.3",
    "symfony/flex": "2.x-dev",
    "symfony/form": "^6.3",
    "symfony/framework-bundle": "6.3.0",
    "symfony/http-client": "^6.3",
    "symfony/http-foundation": "^6.3",
    "symfony/mailer": "^6.3",
    "symfony/maker-bundle": "^1.38",
    "symfony/mime": "^6.3",
    "symfony/monolog-bundle": "^3.14",
    "symfony/process": "^6.3",
    "symfony/property-access": "^6.3",
    "symfony/property-info": "6.3.0",
    "symfony/routing": "^6.3",
    "symfony/runtime": "6.3.0",
    "symfony/security-bundle": "^6.3",
    "symfony/security-csrf": "^6.3",
    "symfony/serializer": "6.3.0",
    "symfony/stopwatch": "^6.3",
    "symfony/string": "^6.3",
    "symfony/translation": "^6.3",
    "symfony/twig-bundle": "v6.3.0",
    "symfony/uid": "6.3.0",
    "symfony/validator": "^6.3",
    "symfony/var-dumper": "^6.3",
    "symfony/web-profiler-bundle": "^6.3",
    "symfony/webpack-encore-bundle": "^2.0.1",
    "symfony/yaml": "6.3.0",
    "twig/cssinliner-extra": "^3.0",
    "twig/extra-bundle": "^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4.4",
    "phpunit/phpunit": "^9.5",
    "symfony/css-selector": "6.3.0",
    "symfony/debug-bundle": "6.3.0",
    "symfony/phpunit-bridge": "^6.3"
  },
  "config": {
    "platform": {
      "php": "8.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  }
}
